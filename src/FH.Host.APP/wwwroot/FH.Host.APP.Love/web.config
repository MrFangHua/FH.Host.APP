<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\.\FH.Host.APP.dll" stdoutLogEnabled="false" stdoutLogFile=".\.\logs\stdout" hostingModel="InProcess" />
	  <defaultDocument>
            <files>
                <add value="fangfang.html" />
            </files>
        </defaultDocument>
    </system.webServer>
  </location>
</configuration>